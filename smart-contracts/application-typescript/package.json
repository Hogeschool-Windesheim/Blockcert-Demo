{
    "name": "living_lab",
    "version": "1.0.0",
    "description": "Asset Transfer Basic contract implemented in TypeScript",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=12",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "pretest": "npm run lint",
        "start": "npm run build && node dist/appCertificateBody.js --configFile resources/certificate-body/config.json --jsonPath configuration",
        "startProducer": "npm run build && node dist/appProducer.js --configFile resources/producer/config.json --jsonPath configuration",
        "startFarmer": "npm run build && node dist/appProducer.js --configFile resources/farmer/config.json --jsonPath configuration",
        "build": "tsc",
        "build:watch": "tsc -w",
        "prepublishbaOnly": "npm run build"
    },
    "engineStrict": true,
    "author": "Hyperledger",
    "license": "Apache-2.0",
    "dependencies": {
        "body-parser": "1.19.0",
        "cors": "2.8.5",
        "express": "^4.17.1",
        "fabric-ca-client": "^2.2.4",
        "fabric-network": "^2.2.4",
        "lodash": "4.17.21",
        "ts-command-line-args": "^1.8.1",
        "tsc": "^1.20150623.0"
    },
    "devDependencies": {
        "@types/body-parser": "1.19.0",
        "@types/cors": "2.8.10",
        "@types/express": "^4.17.11",
        "@types/lodash": "4.14.168",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "coverage/**",
            "dist/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
