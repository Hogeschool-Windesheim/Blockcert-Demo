{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      true,
      "spaces",
      4
    ],
    "interface-name": false,
    "linebreak-style": [
      true,
      "LF"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "no-console": false,
    "curly": true,
    "triple-equals": true,
    "no-string-throw": true,
    "no-var-keyword": true,
    "no-trailing-whitespace": [
      true,
      "ignore-blank-lines"
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "object-literal-shorthand": false,
    "object-literal-sort-keys": false,
    "max-line-length": false,
    "member-access": false,
    "variable-name": {
      "options": "allow-leading-underscore"
    }
  },
  "rulesDirectory": []
}
